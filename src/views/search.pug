extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for #{searchTerm}
        if videos.length === 0
            h5 No videos found
        each video in videos
            +videoBlock({
                id:video.id,
                title:video.title,
                views:video.views,
                videoFile:video.fileUrl,
                creator:video.creator
            })